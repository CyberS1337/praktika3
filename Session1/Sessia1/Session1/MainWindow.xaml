<Window x:Class="Sessia1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sessia1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>
        <Image Source="Resources/logo.jpg" HorizontalAlignment="Right"/>
        <TextBlock Text="Телеком Нева Связь" FontSize="30" VerticalAlignment="Center" Padding="20"/>
        <Frame Grid.Row="1" Name="MainFrame">

        </Frame>
        
    </Grid>
</Window>
